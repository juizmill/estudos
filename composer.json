{
    "name": "zendframework/skeleton-application",
    "description": "Skeleton Application for Zend Framework zend-mvc applications",
    "type": "project",
    "license": "BSD-3-Clause",
    "keywords": [
        "framework",
        "mvc",
        "zf"
    ],
    "homepage": "http://framework.zend.com/",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^5.6 || ^7.0",
        "zendframework/zend-component-installer": "^1.0 || ^0.7 || ^1.0.0-dev@dev",
        "zendframework/zend-mvc": "^3.1",
        "zfcampus/zf-development-mode": "^3.2",
        "zendframework/zend-db": "^2.10",
        "zendframework/zend-mvc-form": "^1.0",
        "zendframework/zend-mvc-console": "^1.2",
        "zendframework/zend-mvc-plugins": "^1.0",
        "zendframework/zend-session": "^2.9",
        "zendframework/zend-servicemanager-di": "^1.2",
        "zendframework/zend-paginator": "^2.8",
        "zendframework/zend-hydrator": "^3.0",
        "zendframework/zend-json": "^3.1",
        "zendframework/zend-mvc-i18n": "^1.1",
        "zendframework/zend-mail": "^2.10",
        "zendframework/zend-authentication": "^2.7",
        "zendframework/zend-math": "^3.2",
        "zendframework/zend-crypt": "^3.3",
        "zendframework/zftool": "dev-master",
        "zendframework/zend-router": "^3.3",
        "zendframework/zend-mvc-plugin-flashmessenger": "^1.2",
        "zendframework/zend-servicemanager": "^3.4"
    },
    "autoload": {
        "psr-4": {

            "Core\\": "module/Core/src/",
            "User\\": "module/User/src/",
            "Application\\": "module/Application/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApplicationTest\\": "module/Application/test/"
        }
    },
    "extra": [],
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "development-disable": "zf-development-mode disable",
        "development-enable": "zf-development-mode enable",
        "development-status": "zf-development-mode status",
        "post-create-project-cmd": [
            "@development-enable"
        ],
        "serve": "php -S 0.0.0.0:8086 -t public public/index.php",
        "test": "phpunit"
    },
    "require-dev": {
        "zendframework/zend-developer-tools": "^1.2"
    }
}
